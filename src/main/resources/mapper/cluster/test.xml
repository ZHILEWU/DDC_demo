<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ddc.cms.dao.cluster.AppUserDao">

<select id="findUser" resultType="Integer"  parameterType="java.lang.String">
		select
		 id
		from app_user
		where username = #{username}
	</select>


</mapper>